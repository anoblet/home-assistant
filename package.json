{
  "name": "@anoblet/home-assistant",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/anoblet/home-assistant#readme",
  "bugs": {
    "url": "https://github.com/anoblet/home-assistant/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anoblet/home-assistant.git"
  },
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "workspaces": [
    "packages\\custom-theme"
  ],
  "scripts": {
    "commit": "wireit",
    "genaiscript": "genaiscript",
    "git:commit": "wireit",
    "git:push": "wireit",
    "sort-package-json": "wireit"
  },
  "devDependencies": {
    "genaiscript": "^1.117.1",
    "sort-package-json": "^2.1.0",
    "wireit": "^0.9.2"
  },
  "wireit": {
    "commit": {
      "dependencies": [
        "git:push"
      ],
      "files": [
        "**/*.js",
        "**/*.yaml"
      ]
    },
    "git:commit": {
      "command": "genaiscript run gcm",
      "dependencies": [
        "sort-package-json"
      ]
    },
    "git:push": {
      "command": "git push",
      "dependencies": [
        "git:commit"
      ]
    },
    "sort-package-json": {
      "command": "sort-package-json",
      "files": [
        "**/package.json"
      ]
    }
  }
}